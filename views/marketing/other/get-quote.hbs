<div class="content-wrap">
    <!-- main page content. the place to put widgets in. usually consists of .row > .col-lg-* > .widget.  -->
    <main id="content" class="content" role="main">
        <!-- Page content -->
        <h1 class="page-title">Find out what your Royalties are worth!</h1>
        <div class="row">
            <div class="col-lg-8">
                <section class="widget">
                    <header>
                        <h4>
                            Input Your Key Information
                        </h4>
                    </header>
                    <div class="widget-body">
                        <form class="form-horizontal" role="form">
                            <fieldset>
                                <legend>Personal Information</legend>
                                <div class="form-group row">
                                    <label for="normal-field" class="col-md-4 form-control-label text-md-right">Full Name</label>
                                    <div class="col-md-7">
                                        <input type="text" id="normal-field" class="form-control" placeholder="">
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label for="hint-field" class="col-md-4 control-label text-md-right">
                                        Email Address
                                    </label>
                                    <div class="col-md-7">
                                        <input type="email" id="hint-field" class="form-control">
                                    </div>
                                </div>
                                <legend>Royalty Information</legend>
                                <div class="form-group row">
                                    <label class="col-md-4 control-label text-md-right" for="tooltip-enabled">Monthly Royality Payment</label>
                                    <div class="col-md-7">
                                        <input type="number" class="form-control">
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-4 control-label text-md-right" for="disabled-input">Basin/Region</label>
                                    <div class="col-md-7">
                                        <select type="text" id="disabled-input" class="form-control" >
                                            <option>Permian</option>
                                            <option>Marcellus</option>
                                            <option>DJ Basin</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-4 control-label text-md-right" for="max-length">Monthly Volumes</label>
                                    <div class="col-md-7">
                                        <input type="number" id="max-length" maxlength="3"
                                               class="form-control">
                                    </div>
                                </div>
                            </fieldset>
                            <div class="form-actions">
                                <div class="row">
                                    <div class="offset-md-4 col-md-7 col-12">
                                        <button type="submit" class="btn btn-primary">Get a Quote Now!</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </section>
            </div>
        </div>
    </main>
</div>
